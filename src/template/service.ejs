                                                                    <% } %>
                                                                    <% if(log.type === 'attempt_force_update' || log.type === 'force_update') { %>
                                                                        <span class="h-8 w-8 rounded-full bg-red-400 flex items-center justify-center ring-8 ring-white dark:ring-gray-800">
                                                                            <svg class="w-5 h-5 text-white" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                                                                                <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm6 10c0 3.31-2.69 6-6 6s-6-2.69-6-6h2c0 2.21 1.79 4 4 4s4-1.79 4-4-1.79-4-4-4v3L8 7l4-4v3c3.31 0 6 2.69 6 6z" fill-rule="evenodd"/>
                                                                            </svg>
                                                                        </span>
                                                                    <% } %>
                                                                    <% if(log.type === 'attempt_scale' || log.type === 'scale') { %>
                                                                        <span class="h-8 w-8 rounded-full bg-blue-400 flex items-center justify-center ring-8 ring-white dark:ring-gray-800">
                                                                            <svg class="w-5 h-5 text-white" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                                                                                <path d="M17,20v-9h-2V4h7l-2,5h2L17,20z M15,13v7H4c-1.1,0-2-0.9-2-2v-3c0-1.1,0.9-2,2-2H15z M6.25,15.75h-1.5v1.5h1.5V15.75z M13,4v7H4c-1.1,0-2-0.9-2-2V6c0-1.1,0.9-2,2-2H13z M6.25,6.75h-1.5v1.5h1.5V6.75z"/>
                                                                            </svg>
                                                                        </span>
                                                                    <% } %>
                                                                    <% if(log.type === 'attempt_restore' || log.type === 'restore') { %>
                                                                        <span class="h-8 w-8 rounded-full bg-green-600 flex items-center justify-center ring-8 ring-white dark:ring-gray-800">
                                                                            <svg class="w-5 h-5 text-white" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                                                                                <path d="M13 3c-4.97 0-9 4.03-9 9H1l3.89 3.89.07.14L9 12H6c0-3.87 3.13-7 7-7s7 3.13 7 7-3.13 7-7 7c-1.93 0-3.68-.79-4.94-2.06l-1.42 1.42C8.27 19.99 10.51 21 13 21c4.97 0 9-4.03 9-9s-4.03-9-9-9zm-1 5v5l4.28 2.54.72-1.21-3.5-2.08V8H12z"></path>
                                                                            </svg>
                                                                        </span>
                                                                    <% } %>
                                                                </div>
                                                                <div class="min-w-0 flex-1 pt-2 flex justify-between space-x-4">
                                                                    <div>
                                                                        <p class="mb-2 text-xs italic text-gray-500 dark:text-gray-400">
                                                                            <%= new Date(log.time).toLocaleTimeString('en-US', { weekday: 'short', month: 'short', day: 'numeric', year: 'numeric', timeZone: 'UTC', timeZoneName: 'short', hour12: false }) %>
                                                                        </p>
                                                                        <% if(log.type === 'attempt_update') { %>
                                                                            <p class="text-xs text-red-800 dark:text-red-400">Attempt to update by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                        <% if(log.type === 'attempt_force_update') { %>
                                                                            <p class="text-xs text-red-800 dark:text-red-400">Attempt to force re-deploy by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                        <% if(log.type === 'attempt_scale') { %>
                                                                            <p class="text-xs text-red-800 dark:text-red-400">Attempt to scale by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                        <% if(log.type === 'attempt_restore') { %>
                                                                            <p class="text-xs text-red-800 dark:text-red-400">Attempt to restore by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>

                                                                        <% if(log.type === 'update') { %>
                                                                            <p class="text-xs text-gray-500 dark:text-gray-400">Image version updated from <%= log.params.old_image_version.split('@')[0] %> to <%= log.params.new_image_version.split('@')[0] %> by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                        <% if(log.type === 'force_update') { %>
                                                                            <p class="text-xs text-gray-500 dark:text-gray-400">Force re-deployed by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                        <% if(log.type === 'scale') { %>
                                                                            <p class="text-xs text-gray-500 dark:text-gray-400">Scaled to <%= log.params.scale %> container(s) by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                        <% if(log.type === 'restore') { %>
                                                                            <p class="text-xs text-gray-500 dark:text-gray-400">Restored by <span class="font-medium text-gray-900 dark:text-gray-100"><%= log.username %></span></p>
                                                                        <% } %>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </li>
                                                <% }); %>
                                            </ul>
                                        </div>
                                        <% if(service_logs.length > 0) { %>
                                            <div>
                                                <a href="/service/<%= service.Spec.Name %>/history/download" download="<%= service.Spec.Name %>_<%= new Date().getTime() %>.log" class="mt-3 inline-flex items-center justify-center w-full block bg-gray-50 dark:bg-gray-700 text-sm font-medium text-gray-500 dark:text-gray-300 text-center px-4 py-4 hover:text-gray-700 hover:dark:text-gray-50 sm:rounded-b-lg">
                                                    <svg class="-ml-1 mr-2 h-6 w-6" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                                                        <path d="M19 9h-4V3H9v6H5l7 7 7-7zM5 18v2h14v-2H5z"/>
                                                    </svg>
                                                    Download Logs
                                                </a>
                                            </div>
                                        <% } %>
                                    </div>
                                </section>
                            </div>
                        </div>
                    </main>

                    <footer class="flex-shrink-0 max-w-7xl w-full mx-auto px-4 sm:px-6 lg:px-8">
                        <nav class="flex justify-center space-x-4 mb-2">
                            <a href="https://github.com/glenndehaan/ship" target="_blank" rel="noopener noreferrer" class="text-sm font-medium hover:text-gray-400">GitHub</a>
                            <span class="inline-block border-l border-gray-300" aria-hidden="true"></span>
                            <a href="https://github.com/glenndehaan/ship/issues" target="_blank" rel="noopener noreferrer" class="text-sm font-medium hover:text-gray-400">Issues</a>
                            <span class="inline-block border-l border-gray-300" aria-hidden="true"></span>
                            <a href="https://github.com/glenndehaan/ship/blob/master/README.md" target="_blank" rel="noopener noreferrer" class="text-sm font-medium hover:text-gray-400">Documentation</a>
                        </nav>
                        <div class="flex justify-center text-xs italic">Host: <%= hostname %></div>
                    </footer>
                </div>
            </div>
        </main>
        <% if(debug_docker) { %>
            <script>
                window.__ship_docker_services = <%- JSON.stringify(docker_services) %>;
                window.__ship_docker_tasks = <%- JSON.stringify(docker_tasks) %>;
            </script>
        <% } %>
        <script>
            if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
                document.querySelector('html').setAttribute('style', 'color-scheme: dark;');
            }

            window.matchMedia('(prefers-color-scheme: dark)').addEventListener('change', event => {
                const newColorScheme = event.matches ? "dark" : "light";

                if(newColorScheme === 'light') {
                    document.querySelector('html').removeAttribute('style');
                } else {
                    document.querySelector('html').setAttribute('style', 'color-scheme: dark;');
                }
            });
        </script>
        <script>
            const params = new URLSearchParams(window.location.search);

            if(params.has('error') || params.has('message')) {
                window.history.replaceState({}, '', window.location.pathname);
            }
        </script>
    </body>
</html>
